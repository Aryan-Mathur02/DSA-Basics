#include <iostream>
using namespace std;
int main()
{
    int key,arr[]={1,2,3,4,5};
    int size=sizeof(arr)/sizeof(int);
    
    cin>>key;
    
    for(int i=0;i<size;i++){
        for(int j=i+1;j<size;j++){
            if(arr[i]+arr[j]==key){
                cout<<arr[i]<<arr[j]<<endl;
            }
        }
    }

    return 0;
}

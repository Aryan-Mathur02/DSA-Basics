#include <iostream>
using namespace std;
int main()
{
    int flag=0;
    
    int arr[]={1,2,3,4,7,6,5};
    int n=sizeof(arr)/sizeof(int);
    
    int brr[]={5,6,7,8};
    int m=sizeof(brr)/sizeof(int);
    
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            if(arr[i]==brr[j]){
                cout<<arr[i]<<" ";
                flag++;
                
            }
        }
    }
    cout<<endl;
    cout<<flag;
    return 0;
}
